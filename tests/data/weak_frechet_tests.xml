<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<tests>
    <test>
        <name>Simple spike example</name>
        <input_a>
            <ipeselection pos="224 480">
                <path>
        96 448 m
        192 448 l
        256 448 l
        320 448 l
        384 448 l
                </path>
            </ipeselection>
        </input_a>
        <input_b>
            <ipeselection pos = "160 448">
                <path>
        96 448 m
        128 448 l
        144 448 l
        192 448 l
        208 512 l
        224 448 l
        256 448 l
        304 448 l
        384 448 l
                </path>
            </ipeselection>
        </input_b>
        <expected_line>
            <ipeselection pos = "160 448">
                <path>
        208 448 m
        208 512 l
                </path>
            </ipeselection>
        </expected_line>
    </test>
    <test>
        <name>Interweaved grid example</name>
        <input_a>
            <ipeselection pos="160 384">
                <path stroke="darkred" pen="heavier" arrow="normal/normal">
        128 384 m
        320 384 l
        320 352 l
        128 352 l
        128 320 l
        320 320 l
        320 288 l
        128 288 l
        128 256 l
        320 256 l
        320 224 l
        128 224 l
        128 192 l
        320 192 l
                </path>
            </ipeselection>
        </input_a>
        <input_b>
            <ipeselection>
                <path>
        128 384 m
        128 192 l
        160 192 l
        160 384 l
        192 384 l
        192 192 l
        224 192 l
        224 384 l
        256 384 l
        256 192 l
        288 192 l
        288 384 l
        320 384 l
        320 192 l
                </path>
            </ipeselection>
        </input_b>
        <expected_line>
            <ipeselection<ipeselection>
                <path>
        352 384 m
        352 192 l
                </path>
            </ipeselection>
        </expected_line>
    </test>
</tests>